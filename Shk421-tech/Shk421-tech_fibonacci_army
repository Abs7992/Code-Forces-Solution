#include<iostream>
using namespace std;
 
int fibonacci(int n)
{
    if(n<=1)
        return n;
 
    return fibonacci(n-1)+fibonacci(n-2);
}
 
int main()
{
    int n;
    cout<<"Enter the value of n"<<endl;
    cin>>n;
 
    cout<<"Required fibonacci number is ";
    cout<<fibonacci(n);
 
    cout<<endl;
    return 0;
}
